#include "StdAfx.h"


#define VERSION ( "v0.1" )
#define PLUGIN_NAMEW ( WSTR( "Olly Extension Plugin" ) )
#define PLUGIN_MODULE_NAMEW ( WSTR( "OllyExt" ) )
#define AUTHOR ( "Ferrit" )
#define AUTHOR_MAIL ( "ferrit.rce@gmail.com" )


void DriverUnload( IN PDRIVER_OBJECT DriverObject )
{
	DbgPrint( "OllyExt : DriverUnload\n" );
}


NTSTATUS DriverEntry( IN PDRIVER_OBJECT DriverObject, IN PUNICODE_STRING RegistryPath )
{
	DbgPrint( "OllyExt : DriverEntry\n" );

	DriverObject->DriverUnload = DriverUnload;

	char buildStr[1024];
	sprintf( buildStr, "OllyExt : %s Build date : %s %s\n", VERSION, __DATE__, __TIME__ );
	DbgPrint( buildStr );
	DbgPrint( "SyserExt : Created by %s\n", AUTHOR );
	DbgPrint( "SyserExt : Send your bugreports/comments to %s\n", AUTHOR_MAIL );
	DbgPrint( "SyserExt : Enjoy :P\n" );

	return STATUS_SUCCESS;
}
